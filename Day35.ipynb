   "source": [
    "#print spacees between \n",
    "S = \"ABC\"\n",
    "# output=\n",
    "# ABC\n",
    "# AB C\n",
    "# A BC\n",
    "# A B C\n",
    "n = (len(S)*2)-1\n",
    "def select(ind,st,n):\n",
    "    if ind==n:\n",
    "        print(st)\n",
    "        return \n",
    "    select(ind+2,st,n)\n",
    "    st = st[:ind]+\"_\"+st[ind:]\n",
    "    select(ind+2,st,n)\n",
    "select(1,S,n)"
   ]
