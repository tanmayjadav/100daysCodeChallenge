   "source": [
    "#Maximum product subarray \n",
    "arr = [1,6,-2,-3,0,5,-2]\n",
    "mx = 0\n",
    "pre = suf = 1\n",
    "n = len(arr)\n",
    "for i in range(n):\n",
    "    pre *=arr[i]\n",
    "    suf *=arr[n-i-1]\n",
    "    if pre==0:\n",
    "        pre = 1\n",
    "    elif suf==0:\n",
    "        suf = 1\n",
    "    if pre>mx or suf>mx:\n",
    "        mx = max(pre,suf)\n",
    "mx"
   ]
